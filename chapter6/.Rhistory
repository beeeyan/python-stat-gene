Calc <- function(x, y, z = 4){
# 複数の値を入力する場合はcにいれる
mini <- min(c(x ,y, z));
medi <- median(c(x, y, z));
maxi <- max(c(x, y, z));
answer <- mini * medi^maxi;
return(answer)
}
